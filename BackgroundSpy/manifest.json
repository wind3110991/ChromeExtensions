{
    "manifest_version": 2,
    "name": "Baidu在线状态",
    "version": "1.0",
    "description": "监视Baidu是否在线",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "img/icon19.png",
            "38": "img/icon38.png"
        }
    },
    "background": {
        "scripts": [
            "spy.js"
        ]
    },
    "permissions": [
        "https://www.baidu.com/"
    ]
}